		 "prefix": "m",
		 "body": [
			 "$$0$",
		 ],
		 "description": "Enter math mode."
	},

	// Made for replacing highlighted text with math mode
	"math.mode.selected": {
		 "prefix": "sm",
		 "body": [
			 "$${TM_SELECTED_TEXT:$1}$ $0",
		 ],
		 "description": "Enter math mode."
	},

	//Made for display mode
	"display.mode": {
		 "prefix": "d",
		 "body": [
