		"body": [
			"\\begin{align*}",
			"\t $0",
			"\\end{align*}",
		],
		"description": ""
	},

	//enumerate environment
	"enumerate": {
		 "prefix": "enum",
		 "body": [
			 "\\begin{enumerate}[${1:1.}]",
			 "\t\\item $0",
			 "\\end{enumerate}"
		 ],
		 "description": "enumerate environment"
	},

	//itemize
	"item": {
