		 ],
		 "description": "Enter display mode."
	},

	//begin environment
	"begin": {
		 "prefix": "beg",
		 "body": [
			 "\\begin{$1}",
			 "\t $0",
			 "\\end{$1}"
		 ],
		 "description": ""
	},

	// Tikzit Figure
	"figure": {
		 "prefix": "tikzfig",
		 "body": [
			"\\begin{figure}[h]",
				"\t\\centering",
				"\t\\tikzfig{$1}",
				"\t\\caption{$2)",
				"\t\\label{fig:c4chi2}",
			"\\end{figure}",
			"$0"
		 ],
		 "description": "place a figure made in tikzit"
	},

