			 "\\textbf{${TM_SELECTED_TEXT:$1}} $0",
		 ],
		 "description": "Write in bold face"
	},

	// Write in bold face
	"math.bold.face": {
		"prefix": "mbf",
		"body": [
			"\\mathbf{${TM_SELECTED_TEXT:$1}} $0",
		],
		"description": "Write in bold face"
   },

	//mathcal
	"math.cal": {
		 "prefix": "mc",
		 "body": [
			 "\\mathcal{${1:P}}$0",
		 ],
		 "description": ""
	},

	//ket
	"math.ket": {
		"prefix": "ket",
		"body": [
			"\\ket{${1:S}}$0",
		],
		"description": ""
	},

